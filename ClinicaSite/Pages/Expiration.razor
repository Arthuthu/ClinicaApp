@page "/expiration"
@inject ILoginService loginService

<div class="text-center fw-bold h2 my-4">
	Sua sessão expirou
</div>

<div class="text-center my-2">
	<a href="/login" class="bg-primary">
		Fazer login
	</a>
</div>

@code {
	protected override async Task OnInitializedAsync()
	{
		await loginService.Logout();
	}
}
